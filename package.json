    "bigint",
    "number"
  ],
  "dependencies": {
    "tiny-webcrypto": "^1.0.1"
  },
  "devDependencies": {
    "benchloop": "^1.3.2",
    "fava": "^0.0.6",
    "tsex": "^1.0.4",
    "typescript": "^4.6.3"
  }
}
